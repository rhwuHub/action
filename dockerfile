# Use a base image that contains Java runtimeFROM openjdk:8-jdk-alpine# Set the working directory in the containerWORKDIR /app# Copy the executable JAR file to the working directoryCOPY /Blog-server/target/main.jar ./app.jar# Command to run the applicationCMD ["java", "-agentlib:jdwp=transport=dt_socket,server=y,suspend=n,address=5005", "-jar", "/app/app.jar"]